<odoo>
    <data>
        <record id="cnt_backup.kota_form" model="ir.ui.view">
            <field name="name">cnt_backup.kota_form</field>
            <field name="model">cnt_backup.kota</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="kota" string="Kota"/>
                            <field name="provinsi" string="Provinsi"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="cnt_backup_kota_action" model="ir.actions.act_window">
            <field name="name">kota</field>
            <field name="res_model">cnt_backup.kota</field>
            <field name="view_mode">tree,form,kanban,graph</field>
        </record>
        
        <record id="cnt_backup_kota_tree" model="ir.ui.view">
            <field name="name">cnt_backup.kota_tree</field>
            <field name="model">cnt_backup.kota</field>
            <field name="arch" type="xml">
                <tree default_order='kota asc' limit="20">
                        <field name="kota" string="kota" optional="show"/>
                        <field name="provinsi" string="Provinsi" optional="show"/>
                </tree>
            </field>
        </record>

